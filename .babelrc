{
  "presets":[
    "react",
    [
      "env",
      {
        "targets":{
          "chrome":54
        }
      }
    ]
  ],
  "plugins":[
    ["transform-object-rest-spread", {
        "useBuiltIns":true
      }
    ],
    ["css-in-js", {
      "vendorPrefixes":true,
      "mediaMap":{
          "phone": "media only screen and (max-width: 768px)",
          "tablet":"media only screen and (max-width: 992px)",
          "desktop":"media only screen and (max-width: 1200px)"
        },
        "transformOptions":{
            "presets":["env"]
        }
      }
    ]
  ]
}
